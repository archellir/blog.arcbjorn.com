{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "test": "deno test --allow-read --allow-env",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "preact": "https://esm.sh/preact@10.27.0",
    "preact/": "https://esm.sh/preact@10.27.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@2.3.1",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.8.0",
    "$std/": "https://deno.land/std@0.224.0/",
    "$gfm": "https://deno.land/x/gfm@0.6.0/mod.ts",
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "js-cookie": "https://esm.sh/js-cookie@3.0.1",
    "url_parse": "https://deno.land/x/url_parse@1.1.0/mod.ts",
    "querystring": "https://deno.land/x/querystring@v1.0.2/mod.js",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "nodeModulesDir": true
}
